{
  "name": "airwr",
  "version": "1.0.0",
  "private": false,
  "license": "MIT",
  "scripts": {
    "start": "vite",
    "build": "cross-env NODE_ENV=production tsc && vite build",
    "pretty": "prettier --check src",
    "lint": "yarn pretty && eslint --max-warnings 0 --ext ts,tsx,json src",
    "generate-css-types": "tailwindcss-classnames -o src/classnames/tailwind.ts && prettier --write \"src/classnames/tailwind.ts\"",
    "prestart": "yarn generate-css-types",
    "prebuild": "yarn generate-css-types",
    "update": "yarn build && git add . && git commit -m 'update' && git push"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@borodutch-labs/localize-react": "^1.7.4",
    "@vitejs/plugin-react": "^1.0.9",
    "daisyui": "^1.16.2",
    "firebase": "^9.4.1",
    "preact": "^10.5.15",
    "react-router-dom": "^6.0.2",
    "unfetch": "^4.2.0",
    "valtio": "^1.2.6",
    "react-editor-js": "^2.0.3",
    "@editorjs/checklist": "^1.3.0",
    "@editorjs/code": "^2.7.0",
    "@editorjs/delimiter": "^1.2.0",
    "@editorjs/editorjs": "^2.22.2",
    "@editorjs/embed": "^2.4.6",
    "@editorjs/header": "^2.6.1",
    "@editorjs/image": "^2.6.1",
    "@editorjs/inline-code": "^1.3.1",
    "@editorjs/link": "^2.3.1",
    "@editorjs/list": "^1.6.2",
    "@editorjs/marker": "^1.2.2",
    "@editorjs/paragraph": "^2.8.0",
    "@editorjs/quote": "^2.4.0",
    "@editorjs/raw": "^2.3.0",
    "@editorjs/simple-image": "^1.4.1",
    "@editorjs/table": "^2.0.1",
    "@editorjs/warning": "^1.2.0"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.1.5",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.34",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "autoprefixer": "^9",
    "cross-env": "7.0.3",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-valtio": "^0.4.1",
    "postcss": "^7",
    "prettier": "^2.4.1",
    "react-scripts": "^4.0.3",
    "rollup-plugin-visualizer": "^5.5.2",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "tailwindcss-classnames": "^2.2.5",
    "typescript": "^4.4.4",
    "vite": "^2.6.14",
    "vite-tsconfig-paths": "^3.3.17"
  }
}
